<div class="md-content md-padding">
    <div class='row'>
        <md-table-container>
            <table md-table>
                <thead md-head>
                    <tr md-row>
                        <th md-column>Note Name</th>
                        <th md-column>Note Description</th>
                        <th md-column>Note Edit Time</th>
                        <th md-column>Download</th>
                    </tr>
                </thead>
                <tbody md-body>
                    <tr md-row md-auto-select ng-repeat="note in vm.noteList">
                        <td md-cell>
                            <div ng-bind-html="note.noteName"></div>
                        </td>
                        <td md-cell>
                            <div ng-bind-html="note.noteDesc"></div>
                        </td>
                        <td md-cell>
                            <div ng-bind-html="note.noteCreateDt"></div>
                        </td>
                        <td md-cell>
                            <md-button class="md-raised md-primary md-fab" type="button" ng-href="{{note.noteLocation}}" download><md-icon md-font-icon="zmdi zmdi-cloud-download"></md-icon></md-button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </md-table-container>
    </div>
</div>
